# Legacy Cloudflare Workers configuration
# This file is kept for reference but the site now deploys to Vercel
# See vercel.json for current deployment configuration

name = "sukisu-ultra-docs-legacy"
main = "src/index.js"
compatibility_date = "2025-08-05"

[site]
bucket = "./docs/.vitepress/dist"

# Note: This configuration is no longer active
# The site is now deployed using Vercel for better performance and global CDN

# Build configuration (legacy)
[build]
command = "npm run docs:build"

# Environment variables (moved to .env files for Vercel)
[vars]
ENVIRONMENT = "legacy"
MIGRATION_NOTE = "Site migrated to Vercel for better performance"
